{

   "Serilog": {
      "MinimumLevel": {
         "Default": "Debug",
         "Override": {
            "Microsoft": "Information",
            "System": "Warning"
         }
      },
      "WriteTo": [
         {
            "Name": "Seq",
            "Args": { "serverUrl": "http://localhost:5341" }
         },

         {
            "Name": "Console"
         }
      ]
   },

   "Logging": {
      "LogLevel": {
         "Default": "Debug"
